    "vue": "^3.5.13",
    "pyloid-js": "<%=pyloidjsVersion%>"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.0.1",
    "run-script-os": "^1.1.6",
    "npm-run-all": "^4.1.5"
  }
}
