{
  "name": "typescript",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel vite pyloid",
    "vite": "vite",
    "pyloid": "uv run -p .venv ./src-pyloid/main.py",
    "build": "vue-tsc -b && vite build && uv run -p .venv ./src-pyloid/build.py",
    "setup": "<%=packageManager%> install && uv venv && uv add -r requirements.txt"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "pyloid-js": "<%=pyloidjsVersion%>"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vue-tsc": "^2.1.10",
    "run-script-os": "^1.1.6",
    "npm-run-all": "^4.1.5"
  }
}