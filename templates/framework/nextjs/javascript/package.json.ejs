{
  "name": "javascript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:next pyloid",
    "dev:watch": "npm-run-all --parallel dev:next pyloid:watch",
    "pyloid": "uv run -p .venv ./src-pyloid/main.py",
    "pyloid:watch": "uv run pyloid-watcher --path ./src-pyloid --pattern \"*.py\" --command \"uv run ./src-pyloid/main.py\"",
    "build": "npm-run-all -s build:next build:pyloid",
    "build:pyloid": "uv run -p .venv ./src-pyloid/build/build.py",
    "setup": "<%=packageManager%> install && uv venv && uv sync",
    "dev:next": "next dev -p 5173 --turbopack",
    "build:next": "next build",
    "start:next": "next start",
    "lint:next": "next lint"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.2.4",
    "pyloid-js": "<%=pyloidjsVersion%>"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "@eslint/eslintrc": "^3",
    "run-script-os": "^1.1.6",
    "npm-run-all": "^4.1.5"
  }
}
