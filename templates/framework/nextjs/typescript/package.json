{
  "name": "typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:next pyloid",
    "pyloid": "uv run -p .venv ./src-pyloid/main.py",
    "build": "build:next && uv run -p .venv ./src-pyloid/build.py",
    "setup": "{package-manager} install && uv venv && uv add -r requirements.txt",
    "dev:next": "next dev -p 5173 --turbopack",
    "build:next": "next build",
    "start:next": "next start",
    "lint:next": "next lint"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.2.4",
    "pyloid-js": "^0.2.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "@eslint/eslintrc": "^3",
    "run-script-os": "^1.1.6",
    "npm-run-all": "^4.1.5"
  }
}
